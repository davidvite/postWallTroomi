{
  "version": 2,
  "buildCommand": "cd packages/client && npm install && npm run build",
  "outputDirectory": "packages/client/dist",
  "functions": {
    "api/**/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/packages/client/dist/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
