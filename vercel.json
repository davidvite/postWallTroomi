{
  "version": 2,
  "buildCommand": "pnpm build:shared && pnpm build:client",
  "outputDirectory": "packages/client/dist",
  "installCommand": "pnpm install",
  "framework": null,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/packages/server/src/server.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/packages/client/dist/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
